{
  "expo": {
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "web": {
      "bundler": "metro"
    },
    "orientation": "portrait",
    "ios": {
      "splash": {
        "image": "./assets/icon/CIC_logo.svg.png",
        "resizeMode": "contain",
        "backgroundColor": "#e5e5e5"
      },
      "android": {
        "splash": {
          "image": "./assets/icon/CIC_logo.svg.png",
          "resizeMode": "contain",
          "backgroundColor": "#e5e5e5"
        }
      },
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true
      },
      "bundleIdentifier": "com.user9510.cvpworkerexporouterapp"
    },
    "name": "cvp-worker-expo-router-app",
    "slug": "cvp-worker-expo-router-app",
    "plugins": ["expo-localization"],
    "locales": {
      "en": "./languages/english.json",
      "zhHK": "./languages/traditionalChinese.json",
      "zhCN": "./languages/simplifiedChinese.json"
    },
    "android": {
      "package": "com.user9510.cvpworkerexporouterapp"
    }
  }
}
